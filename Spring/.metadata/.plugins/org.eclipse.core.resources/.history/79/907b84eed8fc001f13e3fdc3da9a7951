package ConstructorInjection1;

import java.util.List;
import java.util.Map;
import java.util.Set;

public class Employee {

	private int id;
	private String name;
	private double sal;
	private String companay;
	private long phone;
	private List<String> names;
	private Set<Integer> epmid;
	private Map<Integer, String> loc;

	public Employee(int id, String name, double sal, String companay, long phone, List<String> names,
			Set<Integer> epmid, Map<Integer, String> loc) {
		super();
		this.id = id;
		this.name = name;
		this.sal = sal;
		this.companay = companay;
		this.phone = phone;
		this.names = names;
		this.epmid = epmid;
		this.loc = loc;
	}

}
