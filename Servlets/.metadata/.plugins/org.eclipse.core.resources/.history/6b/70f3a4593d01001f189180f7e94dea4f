package com;

public class JspStudent {
	private int id;
	private String name;
	private double mark;
	private int age ;
	
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getMark() {
		return mark;
	}

	public void setMark(double mark) {
		this.mark = mark;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	
	
	@Override
	public String toString(){
		return "ID="+id+","+"Name="+name+","+"Mark="+mark+","+"Age="+age;
	}

}
